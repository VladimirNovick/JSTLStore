<?xml version="1.0" encoding="ISO-8859-1"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" xmlns="http://www.w3.org/1999/xhtml" version="2.0" xmlns:c="http://java.sun.com/jsp/jstl/core">
    <jsp:directive.page contentType="text/html;charset=ISO-8859-1"  pageEncoding="ISO-8859-1" session="false"/>
    <jsp:output omit-xml-declaration="yes" />
    <html>
    <head>
        <title>Online Store</title>
        <link rel="stylesheet" href="Styles/main.css" type="text/css"/>
        <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css"></link>
    </head>
    <body>
    
      
        <div class="jumbotron">
        <h1>Browse Store</h1>
        <p class="lead">Check out our new store categories</p>
      </div>
    
    
    
    <div id="Categories">
    <div class="row">
        <c:set var="store" value="${applicationScope['storeItems']}"/>
        <c:set var='counter' value="0"/>
        <c:forEach items="${store.getCategories()}" var="Cat" >
            <c:set var="counter" value="${counter + 1}"/>
            <c:choose>
                <c:when test="${counter %4 != 0}">
                 <div class="col-lg-3 col-lg-offset-1">
                <img class="img-circle" data-src="holder.js/140x140" alt="140x140" style="width: 140px; height: 140px;" src="data:image/png;base64,"/>
                <div class="container">
                 <h2>${Cat}</h2>
                 <p><a class="btn btn-default" href="Controller?command=viewItemsByCategory&amp;category=${Cat}" role="button">Browse Items</a></p>
                 </div>
                 </div>
                </c:when>
                <c:otherwise>
                    <div class='clearfix'></div>
                </c:otherwise>
            </c:choose>
        </c:forEach>
        </div>
    </div>
    </body>
    </html>
</jsp:root>